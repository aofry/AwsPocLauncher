<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <TextView android:text="@string/hello_world" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/displayData" android:contentDescription="display text"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ClickMe"
        android:id="@+id/getDataBtn"
        android:layout_below="@+id/displayData"
        android:layout_alignParentStart="true"
        android:layout_marginTop="61dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Launch AUT"
        android:id="@+id/launchBtn"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/editMe" android:layout_below="@+id/getDataBtn" android:layout_alignParentEnd="true"
            android:layout_marginEnd="110dp" android:contentDescription="editMe" android:text="aaa"/>

</RelativeLayout>
